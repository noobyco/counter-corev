v0x55578413c150_0 .net "up_down", 0 0, v0x55578413c810_0;  1 drivers
E_0x555784122d00 .event posedge, v0x5557841226f0_0;
E_0x555784123010 .event posedge, v0x55578413c090_0, v0x5557841226f0_0;
    .scope S_0x555784122420;
T_0 ;
    %wait E_0x555784123010;
    %load/vec4 v0x55578413c090_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55578413bd20_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55578413bec0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55578413c150_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x55578413bd20_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55578413bd20_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x55578413bd20_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55578413bd20_0, 0;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x555784122420;
T_1 ;
    %wait E_0x555784122d00;
    %load/vec4 v0x55578413bf80_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55578413be00_0;
    %assign/vec4 v0x55578413bd20_0, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55578410f7b0;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55578410f7b0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55578413c2f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55578413c740_0, 0, 1;
