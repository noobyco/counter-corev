    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55578413c740_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55578413c580_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55578413c810_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55578413c650_0, 0, 1;
    %delay 20, 0;
