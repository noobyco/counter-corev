    .port_info 3 /INPUT 1 "up_down";
    .port_info 4 /INPUT 1 "load";
    .port_info 5 /INPUT 4 "data_in";
    .port_info 6 /OUTPUT 4 "count";
v0x5557841226f0_0 .net "clk", 0 0, v0x55578413c2f0_0;  1 drivers
v0x55578413bd20_0 .var "count", 3 0;
v0x55578413be00_0 .net "data_in", 3 0, v0x55578413c480_0;  1 drivers
